{
  "name": "generator-ftb",
  "version": "1.0.0",
  "description": "A simple, but powerful code generator for flutter projects using bloc pattern",
  "homepage": "https://github.com/marcoprado17/generator-ftb",
  "author": {
    "name": "Marco Prado",
    "email": "marco.prado.public@gmail.com",
    "url": "mprado.me"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "flutter",
    "bloc",
    "bloc pattern arhictecture",
    "bolerplate",
    "bloc boilerplate",
    "performance",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^26.1.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.11.0",
    "chalk": "^4.1.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "marcoprado17/generator-ftb",
  "license": "MIT"
}
